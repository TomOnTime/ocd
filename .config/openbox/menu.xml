<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="ELITE HACKING MENU">
  <item label="gnome-terminal">
    <action name="Execute">
      <execute>gnome-terminal --geometry=160x45+75+50</execute>
    </action>
  </item>
  <item label="chrome (ksw@google.com)">
    <action name="Execute">
      <execute>google-chrome --proxy-pac-url=http://wpad.corp.google.com/wpad.dat</execute>
    </action>
  </item>
  <item label="chrome (obeyeater@gmail.com)">
    <action name="Execute">
      <execute>google-chrome --user-data-dir=/home/ksw/.config/google-chrome/Personal</execute>
    </action>
  </item>
  <separator />
  <menu id="display" label="Displays">
    <item label="DVI-0">
      <action name="Execute">
        <execute>xrandr --output DVI-0 --primary --auto --output LVDS --off --output VGA-0 --off</execute>
      </action>
    </item>
    <item label="LVDS">
      <action name="Execute">
        <execute>xrandr --output DVI-0 --off --output LVDS --primary --auto --output VGA-0 --off</execute>
      </action>
    </item>
    <item label="VGA-0">
      <action name="Execute">
        <execute>xrandr --output DVI-0 --off --output LVDS --off --output VGA-0 --primary --auto</execute>
      </action>
    </item>
    <item label="All">
      <action name="Execute">
        <execute>xrandr --auto</execute>
      </action>
    </item>
  </menu>
  <menu id="client-list-menu" />
  <separator />
  <item label="ObConf">
    <action name="Execute"><execute>obconf</execute></action>
  </item>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Restart">
    <action name="Restart" />
  </item>
  <separator />
  <item label="Exit">
    <action name="Exit" />
  </item>
</menu>

</openbox_menu>
