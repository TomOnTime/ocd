<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="ELITE HACKING MENU">

  <item label="chrome">
    <action name="Execute">
      <execute>
        google-chrome
      </execute>
    </action>
  </item>

  <item label="gnome-terminal">
    <action name="Execute">
      <execute>gnome-terminal --geometry=160x45+75+50</execute>
    </action>
  </item>

  <item label="gnome-control-center">
    <action name="Execute">
      <execute>
        gnome-control-center
      </execute>
    </action>
  </item>

  <separator />

  <menu id="displays" label="Displays" execute="~/.config/openbox/pipemenus/displays" />

  <menu id="client-list-combined-menu" />

  <separator />

  <menu id="Debian" />

  <item label="ObConf">
    <action name="Execute"><execute>obconf</execute></action>
  </item>

  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>

  <item label="Restart Openbox">
    <action name="Restart" />
  </item>

  <separator />

  <item label="Exit Openbox">
    <action name="Exit" />
  </item>

  <separator />

  <item label="Shutdown System">
    <action name="Execute">
      <execute>
        gdm-control --shutdown
      </execute>
    </action>
  </item>

  <item label="Restart System">
    <action name="Execute">
      <execute>
        gdm-control --reboot
      </execute>
    </action>
  </item>

</menu>

</openbox_menu>
