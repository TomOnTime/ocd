#!/bin/sh

personal() {
  exec google-chrome \
      --user-data-dir=/usr/local/home/e/.config/google-chrome/Personal
}

work() {
  exec google-chrome \
      --proxy-pac-url=http://wpad.corp.google.com/wpad.dat \
      --user-data-dir=/usr/local/google/home/ksw/.config/google-chrome/Work
}

case $1 in
  personal)
    personal
    ;;
  work)
    work
    ;;
  *)
    personal
    ;;
esac
